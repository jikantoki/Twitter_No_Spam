{
  "name": "Twitter No Spam",
  "description": "Twitter(X)のスパムコメントを排除",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "http://twitter.com/*",
        "https://twitter.com/*",
        "http://x.com/*",
        "https://x.com/*"
      ],
      "js": ["twitterNoSpam.js"],
      "css": ["isSpam.css"]
    }
  ]
}
